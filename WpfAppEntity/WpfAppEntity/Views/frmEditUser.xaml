<Window x:Class="WpfAppEntity.Views.frmEditUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppEntity.Views"
        mc:Ignorable="d"
        Title="frmEditUser" Height="200" Width="300" Loaded="Window_Loaded">
    <Grid Margin="0,0,0,0">
        <TextBox Name="tbxName" HorizontalAlignment="Left" Height="23" Margin="30,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="230"/>
        <TextBox Name="tbxLogin" HorizontalAlignment="Left" Height="23" Margin="30,58,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="230"/>
        <TextBox Name="tbxSenha" HorizontalAlignment="Left" Height="23" Margin="30,86,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="230"/>
        <Button Name="btnSalvar" Click="BtnSalvar_Click" Content="Salvar" FontSize="22" FontFamily="Comic Sans MS" Margin="30,0,33.6,10.4" Height="39" VerticalAlignment="Bottom"/>

    </Grid>
</Window>
